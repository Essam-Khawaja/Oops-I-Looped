script/source = "extends TileMap

var gridSize = 4
var Dic = {}
var selectedTile

func _ready() -> void:
	for x in gridSize:
		for y in gridSize:
			Dic[str(Vector2(x, y))] ={
				\"Type\": \"Dirt\",
				\"Position\": str(Vector2(x, y))
			}
			set_cell(0, Vector2(x, y), 0, Vector2i(0, 0), 0)
	print(Dic)
	
func _process(delta: float) -> void:
	var mouseTile = local_to_map(get_global_mouse_position())
	selectedTile = map_to_local(mouseTile)
	
	for x in gridSize:
		for y in gridSize:
			erase_cell(1, Vector2(x, y))		# Erase all the selected tiles
	
	if Dic.has(str(mouseTile)):
		set_cell(1, mouseTile, 1, Vector2i(0, 0), 0)
		print(Dic[str(mouseTile)].Position)
"

